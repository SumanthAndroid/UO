<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/item_add_on_select_time_container"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/state_list_background_select_time"
    android:orientation="horizontal"
    android:paddingBottom="12dp"
    android:paddingTop="12dp">


    <com.universalstudios.orlandoresort.view.fonts.TextView
        android:id="@+id/item_add_on_select_time_title"
        style="@style/TextViewStyle.GothamMedium"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:textAllCaps="true"
        android:textColor="@color/state_list_select_sku_selector_text"
        android:textSize="16sp"
        tools:text="9:00"/>

</RelativeLayout>
